{
  "kind": "collectionType",
  "collectionName": "apim_configs",
  "info": {
    "name": "ApimConfig",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "url": {
      "type": "string"
    },
    "clientId": {
      "type": "string"
    },
    "clientSecret": {
      "type": "string"
    },
    "apiToken": {
      "type": "string"
    },
    "active": {
      "type": "boolean"
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "organizationId": {
      "type": "string"
    },
    "platform": {
      "type": "enumeration",
      "enum": [
        "MuleSoft",
        "Azure"
      ],
      "required": true,
      "default": "MuleSoft"
    }
  }
}
