{
  "kind": "collectionType",
  "collectionName": "library_apis",
  "info": {
    "singularName": "library-api",
    "pluralName": "library-apis",
    "displayName": "LibraryApi",
    "name": "LibraryApi",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "description": {
      "type": "text"
    },
    "color_status": {
      "type": "string"
    },
    "footerText": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "allowedTypes": ["images", "files", "videos"],
      "multiple": true,
      "required": false
    },
    "version": {
      "type": "string"
    },
    "buttons": {
      "type": "component",
      "repeatable": true,
      "component": "links.button"
    },
    "tags": {
      "type": "component",
      "repeatable": true,
      "component": "elements.titles"
    },
    "openDocType": {
      "type": "enumeration",
      "enum": ["api", "asyncapi"],
      "required": true,
      "default": "api"
    },
    "openDocFormat": {
      "type": "enumeration",
      "enum": ["json", "yaml"],
      "required": true,
      "default": "json"
    },
    "openDoc": {
      "type": "customField",
      "customField": "plugin::strapi-code-editor-custom-field.code-editor-text",
      "options": {
        "language": "plaintext"
      }
    },
    "markdown": {
      "type": "richtext"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "library_apis"
    },
    "definitionRating": {
      "type": "enumeration",
      "enum": ["A", "B", "C", "D", "E"]
    },
    "securityRating": {
      "type": "enumeration",
      "enum": ["A", "B", "C", "D", "E"]
    },
    "qualityRating": {
      "type": "enumeration",
      "enum": ["A", "B", "C", "D", "E"]
    },
    "aiReady": {
      "type": "boolean",
      "default": false
    },
    "publish": {
      "type": "enumeration",
      "enum": [
        "publicado",
        "noPublicado"
      ]
    }
  }
}
